# Environment Variables Template
# Copy this file to .env.development or .env.production and fill in your values

# Backend API URL
REACT_APP_BACKEND_URL=

# Calendar email
REACT_APP_CALENDAR_EMAIL=

# Site URL
REACT_APP_SITE_URL=

# Audit booking URL - MUST be Google Calendar Appointment Schedule only
# Generated from israel.growth.venture@gmail.com Google account
# Example: https://calendar.google.com/calendar/appointments/schedules/...
REACT_APP_AUDIT_BOOKING_URL=

# Audit payment URL (optional - currently unused, payment done via internal /payment?pack=audit)
REACT_APP_AUDIT_PAYMENT_URL=

# API Timeout (milliseconds)
REACT_APP_API_TIMEOUT=30000

# Feature flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PDF_DOWNLOAD=true
REACT_APP_ENABLE_PDF_EMAIL=true
